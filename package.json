{
  "name": "ng-styleguide",
  "version": "0.0.1",
  "description": "Living styleguide in an angular 2 webapp",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --inline",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "serve:build": "http-server ./dist",
    "postinstall": "typings install",
    "sw": "npm shrinkwrap --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KBizien/ng-styleguide.git"
  },
  "keywords": [
    "styleguide",
    "angular",
    "components"
  ],
  "author": "kbizien",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KBizien/ng-styleguide/issues"
  },
  "homepage": "https://github.com/KBizien/ng-styleguide#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
